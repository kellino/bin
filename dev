#!/bin/sh

transset=$(which transset-df 2>/dev/null)
tmux=$(which tmux 2>/dev/null)
#mutt=$(which mutt)
#ranger=$(which ranger)

if [ -x "$transset" ];  then
    transset-df -a 0.75
fi

if [ ! -x "$tmux" ]; then
    exit 
else
    tmux
    tmux set-option -ga terminal-overrides ",xterm-256color:Tc"
fi
